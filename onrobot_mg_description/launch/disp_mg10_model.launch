<?xml version="1.0" ?>
<launch>
  <arg name="gui" default="True" />
  <param name="robot_description" command="$(find xacro)/xacro $(find onrobot_mg_description)/urdf/onrobot_mg10.xacro" />
  <param name="use_gui" value="$(arg gui)"/>
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen" />
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find onrobot_mg_description)/config/mg10_visualize.rviz" required="true" />
</launch>
